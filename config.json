{
	"model":{
                "name":"pascalvoc2017",
                "input_size": 320,
                "classes":80,
                "backbone": "vgg16",
                "backbone_feature1_size": [40, 40],
                "backbone_feature2_size": [20, 20],
                "scales": 5,
                "tums" : 8

	},

    "anchors":{
                  "scales":  3,
                  "aspect_ratio": [[1, 1], [1.41, 0.71], [0.71, 1.41]],
                  "iou_thresh": 0.6,
                  "num_anchors":9
    },

	"train":{
                "images_dir":   "datasets/minicoco/",
                "labels_dir":   "datasets/minicoco/",     
                "pretrained_weights":   "",
                "batch_size":           3,
                "learning_rate":        1e-4,
                "nb_epochs":            500,
                "saved_weights_name":   "coco2017.h5",
                "model_dir":            "saved_model/",
                "debug":                true,
                "pre_nms_thresh":       0.5,
                "post_nms_thresh":      0.5
	}
}